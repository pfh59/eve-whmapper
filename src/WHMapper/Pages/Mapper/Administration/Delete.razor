<MudDialog>
    <DialogContent>
        @if (AccessId > 0)
        {
            <MudText Class="pointer-events-none">@MSG_ACCESS_DELETE</MudText>
        }

        @if (AdminId > 0)
        {
            <MudText Class="pointer-events-none">@MSG_ADMIN_DELETE</MudText>
        }
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancel</MudButton>
        <MudButton Color="Color.Error" Variant="Variant.Filled" OnClick="Submit">Delete</MudButton>
    </DialogActions>
</MudDialog>

