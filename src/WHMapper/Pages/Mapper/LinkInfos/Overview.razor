<MudPaper Outlined="true" hidden="@(CurrentSystemLink == null)" class="d-flex flex-column justify-space-between">
    <MudStack class="pa-2" Spacing="1">
        <MudStack Justify="Justify.SpaceBetween">
            <MudText Typo="Typo.body2" Style="font-weight:bold;">First Jump:</MudText>
            <MudText Typo="Typo.body2">By @(FirstJumpLogCharacterName)</MudText>
            <MudText Typo="Typo.body2">at @(FirstJumpLogDate)</MudText>
        </MudStack>
    </MudStack>
</MudPaper>

